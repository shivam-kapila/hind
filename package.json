{
  "name": "hind",
  "version": "1.0.0",
  "description": "A platform to provide a glimpse into Hindustan's rich cultute and tradition.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shivam-kapila/hind.git"
  },
  "scripts": {
    "build:dev": "webpack --mode=development --watch --progress --color",
    "build:prod": "webpack --mode=production --no-watch --progress --color",
    "build:less": "lessc /static/css/main.less > /static/css/main.css",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:dev",
    "format": "eslint ./static/js/src --ext .js,jsx,ts,tsx --fix --quiet",
    "type-check": "tsc --noEmit"
  },
  "author": "Shivam Kapila",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shivam-kapila/hind/issues"
  },
