    "@babel/preset-typescript": "^7.13.0",
    "@fortawesome/fontawesome-common-types": "^0.2.35",
    "@types/clean-css": "^4.2.4",
    "@types/less": "^3.0.2",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.7",
